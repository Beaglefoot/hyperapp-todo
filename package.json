{
  "name": "hyperapp-todo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "test:watch": "npm run test -- --watch --watch-extensions jsx",
    "build": "webpack --config ./config/webpack.prod.config.js",
    "start": "webpack-dev-server --config ./config/webpack.dev.config.js",
    "analyze": "webpack --config ./config/webpack.analyze.config.js",
    "serve": "npm-run-all --parallel serve:watch serve:serve",
    "serve:debug": "APP_DEBUG=true yarn serve",
    "serve:serve": "[ -d dist ] || mkdir dist; serve dist -p 7000 -n",
    "serve:watch": "webpack --config ./config/webpack.serve.config.js -w",
    "fix:assets":
      "prettier --ignore-path .gitignore --write \"**/*.{json,md,scss}\"",
    "fix:code": "yarn lint:code -- --fix",
    "fix": "yarn fix:code && yarn fix:assets",
    "lint:code":
      "eslint --ignore-path .gitignore --ignore-path .prettierignore \"**/*.{js,jsx}\"",
    "lint": "yarn lint:code"
  },
  "lint-staged": {
    "*.{json,scss,md}": ["prettier --write", "git add"],
    "*.{js,jsx}": ["eslint --fix --ignore-path .prettierignore", "git add"]
  },
  "author": "Beaglefoot",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.1",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.9",
    "date-fns": "^1.28.5",
    "es3ify-webpack-plugin": "^0.0.1",
    "eslint": "4.17.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.29.0",
    "husky": "0.14.3",
    "image-webpack-loader": "^4.0.0",
    "lint-staged": "6.1.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.2",
    "prettier": "1.10.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "@hyperapp/logger": "^0.4.1",
    "cuid": "^2.1.0",
    "hyperapp": "^1.1.2"
  }
}
