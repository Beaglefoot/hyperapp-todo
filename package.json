{
  "name": "hyperapp-todo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "test:watch": "npm run test -- --watch --watch-extensions jsx",
    "build": "webpack-cli --config ./config/webpack.prod.config.js",
    "start": "webpack-dev-server --config ./config/webpack.dev.config.js",
    "analyze": "webpack-cli --config ./config/webpack.analyze.config.js",
    "fix:assets": "prettier --ignore-path .gitignore --write \"**/*.{json,md,scss}\"",
    "fix:code": "yarn lint:code -- --fix",
    "fix": "yarn fix:code && yarn fix:assets",
    "lint:code": "eslint --ignore-path .gitignore --ignore-path .prettierignore \"**/*.{js,jsx}\"",
    "lint": "yarn lint:code"
  },
  "lint-staged": {
    "*.{json,scss,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix --ignore-path .prettierignore",
      "git add"
    ]
  },
  "author": "Beaglefoot",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-minify-webpack-plugin": "0.3.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "0.28.11",
    "date-fns": "^1.28.5",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.1.0",
    "husky": "0.14.3",
    "image-webpack-loader": "4.2.0",
    "lint-staged": "7.0.0",
    "node-sass": "4.8.3",
    "prettier": "1.11.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "6.0.7",
    "style-loader": "0.20.3",
    "webpack": "4.4.1",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "3.1.1",
    "webpack-merge": "4.1.2"
  },
  "dependencies": {
    "@hyperapp/logger": "0.5.0",
    "cuid": "^2.1.0",
    "hyperapp": "1.2.5"
  }
